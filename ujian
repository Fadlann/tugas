    int a = 0;
    int b = 5;
    int c = 9;
    a = b + 3;
    b = c - 1;
    cout<<a<<endl;
    cout<<b<<endl;
    while( a != b )
    {
        c = c - 2;
        if ( a > b )
        {
            a = a - b;
        }
        else
        {
            b = c - a;
        }
    }
    cout<<a<<endl;
    cout<<b<<endl;
}
